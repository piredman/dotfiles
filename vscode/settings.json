{
  "files.autoSave": "off",
  "editor.renderWhitespace": "boundary",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.handleKeys": {
    "<C-f>": false,
    "<C-c>": false,
    "<C-v>": false,
    "<C-x>": false,
    "<C-h>": false,
    "<C-a>": false
